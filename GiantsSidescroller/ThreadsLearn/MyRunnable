package ThreadsLearn;

import static ThreadsLearn.ThreadColor.ANSI_RED;

public class MyRunnable implements Runnable{

    public MyRunnable() {
      //  System.out.println("I am a hunter");
    }

    @Override
    public void run() {
        System.out.println(ANSI_RED+ "Hello world i am from the myrunnable class");


    }
}
